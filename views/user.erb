<h1><%= @name.capitalize %></h1>

<h3>Email</h3>
<ul>
  <li>
    <%= @email %>
  </li>
</ul>

<h3>Interests</h3>
<ul>
  <li>
    <%= @interests %>
  </li>
</ul>

<p>Other Users</p>
<ul>
  <% @other_users.each do |name| %>
    <li>
      <a href="<%= name %>">
        <%= name.capitalize %>
      </a>
    </li> 
  <% end %>
</ul>